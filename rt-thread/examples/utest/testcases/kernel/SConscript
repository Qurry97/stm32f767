Import('rtconfig')
from building import *

cwd     = GetCurrentDir()
src     = Split('''
''')

if GetDepend(['UTEST_MEMHEAP_TC']):
    src += ['memheap_tc.c']

if GetDepend(['UTEST_SEMAPHORE_TC']):
    src += ['semaphore_tc.c']

CPPPATH = [cwd]

group = DefineGroup('utestcases', src, depend = [], CPPPATH = CPPPATH)

Return('group')
